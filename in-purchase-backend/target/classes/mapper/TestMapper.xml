<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="org.example.mapper.TestMapper">
    <insert id="insert" parameterType="org.example.model.Order">
        insert into test1(id,date) values (#{o.id},#{o.time})
    </insert>
    <select id="selectAll" resultType="org.example.model.Order">
        select * from test1;
    </select>
</mapper>
